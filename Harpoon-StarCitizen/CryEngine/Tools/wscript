SUBFOLDERS = [
	'CryTIFPlugin',
	'CryXML',
	'MaxCryExport',
	'MayaCryExport2',
	'MBCryExport',
	'RC',
	'ShaderCacheGen',
	'PakEncrypt',
	'HLSLCrossCompiler'
]
	
def build(bld):
	# Recursive into all sub projects
	bld.recurse(SUBFOLDERS)
